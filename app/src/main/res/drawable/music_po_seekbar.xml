<?xml version="1.0" encoding="utf-8"?>
<layer-list
    xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="0.5dp"/>
            <solid android:color="@color/color_commWhite" />
            <stroke android:color="#fef0e5" android:width="1dip"/>
        </shape>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <solid android:color="#ff51495e" />
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="0.5dp"/>
                <gradient
                    android:startColor="#ffe1cc"
                    android:centerColor="#f2ae91"
                    android:endColor="#ffe1cc"
                    android:angle="270" />
            </shape>
        </clip>
    </item>
</layer-list>